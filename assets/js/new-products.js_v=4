jQuery(document).ready(function ($) {
    var item_count = $(".owl-carousel1 .owl-carousel-item").length;
    $(".owl-carousel1").owlCarousel({dots:!1,nav:!0,responsive:{0:{loop:!0,items:1,margin:0,stagePadding:10},370:{loop:!0,items:2,margin:20,stagePadding:10},550:{loop:!0,items:3,margin:20,stagePadding:10},850:{loop:!0,items:4,margin:20,stagePadding:10},1200:{loop:!1,items:item_count,stagePadding:10,margin:20}}});
    $(".owl-carousel-feature").owlCarousel({dots:!0,nav:!0,responsive:{0:{loop:!0,items:1,margin:0,stagePadding:0}}});
    $(".addToCartBtn").on("click", function (e) {
        e.preventDefault();
        var btn = $(this);
		$.ajax({
			url:add_to_cart_url,
			data:{ product_id:btn.attr('data-product-id'), product_name:btn.attr('data-product-name'), product_freq:btn.attr('data-product-freq'), product_bump:btn.attr('data-product-bump'), product_ads:btn.attr('data-product-ads'), product_qty:btn.attr('data-product-qty') },
			type:"POST",
			dataType:"json",
			success:function (rez) {
				if(rez.success){
					$("#cartCountDisplay").text(rez.cart_item_count);
					window.location.href = "/cart";
				}
			}
		})
    });
    var feq_total = 0;
    $.when($(".feq-bought-checkbox").each(function(){
        feq_total += parseFloat($(this).attr('data-price'));
    }).on("change", function () {
        var sub = $(this);
        var product_id = sub.val();
        if(!sub.is(":checked")){
            $(".feq-bought-items").find("img[data-product-id='"+product_id+"']").addClass('grey-it');
            feq_total -= parseFloat($(this).attr('data-price'));
        }else{
            $(".feq-bought-items").find("img[data-product-id='"+product_id+"']").removeClass('grey-it');
            feq_total += parseFloat($(this).attr('data-price'));
        }
        $("#boughtTogetherPrice").text(feq_total);
    })).then(function(){
      $("#boughtTogetherPrice").text(feq_total);
    });
    $("#boughtTogehterBtn").on("click", function () {
        var products = [];
        $(".feq-bought-checkbox").each(function () {
            if($(this).is(":checked")){
                products.push({product_id:$(this).val(), product_qty:1});
            }
        });
        if(products.length == 0){
            Swal.fire({
                type: 'error',
                title: 'Add To Cart Error',
                html: "<b>No Products Selected</b>"
            });
            return false;
        }
        $.ajax({
            url:add_to_cart_multi_url,
            data:{ products:products },
            type:"POST",
            dataType:"json",
            success:function (rez) {
                if(rez.success){
                    $("#cartCountDisplay").text(rez.cart_item_count);
                    window.location.href = "/cart";
                }
            }
        })
    });
    $(".thumbnail").on("click", function () {
        var img = $(this).attr("data-image");
        $(".thumbnails li").each(function () {
            $(this).removeClass("active");
        });
        $(this).parent().addClass("active");
        var display = $(this).attr("data-display-type") == "image" ? "<img class=\"d-block\" src=\""+$(this).attr("data-display")+"\" alt=\""+$(this).attr("alt")+"\">" : $(this).attr("data-display");
        $("#featureImageCon").html(display);
    });
    $(window).on('orientationchange', function () {
        // alert("as");
        // $(".owl-carousel-feature").trigger('destroy.owl.carousel');
        $(".owl-carousel-feature").owlCarousel({dots:!0,nav:!0,responsive:{0:{loop:!0,items:1,margin:0,stagePadding:0}}});
    });



    //ads=ecomm&tcode=get22offemail

    var url_string = window.location.href;
    var url = new URL(url_string);
    var ads = url.searchParams.get("ads");
    var tcode = url.searchParams.get("tcode");
//    console.log(ads);
//    console.log(tcode);
});
